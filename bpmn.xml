                    <manipulate id="a11" label="rule and message deletion">count_msg_deletion = 0
count_rule_deletion = 0

data.matching_results.each { |match|
  match["messages"].each { |match_msg|    id = match_msg["id"]
    data.messages.each_with_index{ |msg, index|
      if msg["id"] == id
        data.messages.delete_at(index)
        count_msg_deletion += 1
      end
    }

  }
  data.rules.each_with_index{ |rule, index|
    if rule["id"] == match["id"]
      data.rules.delete_at(index)
      count_rule_deletion += 1
    end
  }
}

data.len_messages -= count_msg_deletion
data.len_rules -= count_rule_deletion

data.matching_results = []</manipulate>
